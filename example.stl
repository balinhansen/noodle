# Facets on the Z-plane

facet normal 0.0E+0,0.0E+0,1.0E+0
  outer loop
    vertex 0.0E+0,0.0E+0,1.0E+0
    vertex 1.0E+0,0.0E+0,1.0E+0
    vertex 0.0E+0,1.0E+0,1.0E+0
  endloop
endfacet

facet normal 0.0E+0,0.0E+0,1.0E+0
  outer loop
    vertex 1.0E+0,0.0E+0,1.0E+0
    vertex 1.0E+0,1.0E+0,1.0E+0
    vertex 0.0E+0,1.0E+0,1.0E+0
  endloop
endfacet


facet normal 0.0E+0,0.0E+0,-1.0E+0
  outer loop
    vertex 0.0E+0,0.0E+0,0.0E+0
